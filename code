// textC++.cpp : Этот файл содержит функцию "main". Здесь начинается и заканчивается выполнение программы.
//
#include <fstream>
#include <string.h>
#include <iostream>
using namespace std;

int main()
{
	
	int k, l, i, j, N, M, a[20][20], b[20][20], c[20][20];
	cout << "N="; //ввод количества строк
	cin >> N;
	M = N;
	cout << "Input matrix A \n";
	//цикл по переменной i, в которой перебираем строки матрицы
	for (i = 0; i < N; i++)
		//цикл по переменной j, в котором перебираем элементы внутри строки
		for (j = 0; j < M; j++)
			cin >> a[i][j];//ввод очередного элемента матрицы
	cout << "Input matrix B" << endl;//цикл по переменной i, в которой перебираем строки матрицы
	for (i = 0; i < N; i++)
		//цикл по переменной j, в котором перебираем элементы внутри строки
		for (j = 0; j < M; j++)
			cin >> b[i][j]; //ввод очередного элемента матрицы

	cout << "matrix A \n";
	for (i = 0; i < N; i++)
	{
		//цикл по переменной i, в котором перебираем строки матрицы
		for (j = 0; j < M; j++)
			cout << a[i][j] << "\t"; //вывод очередного элемента матрицы
		cout << endl; //переход на новую строку после вывода всех элементов строки
	}

	cout << "matrix B \n";
	for (i = 0; i < N; i++)
	{
		//цикл по переменной i, в котором перебираем строки матрицы
		for (j = 0; j < M; j++)
			cout << b[i][j] << "\t"; //вывод очередного элемента матрицы
		cout << endl; //переход на новую строку после вывода всех элементов строки
	}
	i = 0;
	j = 0;
	//c = 0;
	for (i = 0; i < N; i++)
	{

		for (j = 0; j < N; j++)
		{
			c[i][j] = 0;
			for (int k = 0; k < N; k++)
				c[i][j] += a[i][k] * b[k][j];
		}

	}

	cout << "matrix A \n";
	for (i = 0; i < N; i++)
	{
		//цикл по переменной i, в котором перебираем строки матрицы
		for (j = 0; j < M; j++)
			cout << c[i][j] << "\t"; //вывод очередного элемента матрицы
		cout << endl; //переход на новую строку после вывода всех элементов строки
	}
	//system("pause");
	return 0;
}
