double integralpram(double a, double b, int n)
{
	double h, S, x;
	int i;
	h = (b - a) * 1.0 / n;
	S = 0;
	for (i = 0; i < n - 1; i++)
	{
		x = a + i * h;
		S = S + (1 + sin(x) / (1 + cos(x)));
	}
	S = h * S;
	return S;
}*/
int main()
{
	using namespace std;
	double y, a, b, k;
	int n;
	cin >> a;
	cin >> b;
	cin >> n;
	cin >> k;

	//y = integralpram(a, b, n);
	y = trap(a, b, n);
	cout << y << "\n";
	y = pryam(a, b, n, k);
